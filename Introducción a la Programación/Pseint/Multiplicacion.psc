//Definición del proceso o programa
Algoritmo Multiplicacion
	//Entradas
	Definir a, b, c, result Como Real;
	//Instrucción al usuario
	Escribir "Digite un numero 1:";
	Leer a;
	//Instrucción al usuario
	Escribir "Digite un numero 2:";
	Leer b;
	//Instrucción al usuario
	Escribir "Digite un numero 3:";
	Leer c;
	//Proceso
	result = a + b + c;
	//Salida
	Escribir "El resultado es: ", result;
FinAlgoritmo
